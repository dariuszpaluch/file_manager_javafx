<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.dariuszpaluch.FilesTableView?>
<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.dariuszpaluch.FilesBrowserController">
    <FlowPane>
        <Button fx:id="goUpButton" text="GoUpButton" />
        <Button fx:id="refreshButton" text="Refresh" />
        <ChoiceBox fx:id="driveChoiceBox" />
    </FlowPane>
    <Text text="currentPath" fx:id="currentPathText"/>

    <FilesTableView fx:id="filesTableView" HBox.hgrow="ALWAYS"/>

</VBox>
